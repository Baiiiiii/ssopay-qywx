	<build>
		<finalName>ssopay-qywx-admin</finalName>
		<plugins>
	      <plugin>
	         <groupId>org.apache.maven.plugins</groupId>
	         <artifactId>maven-war-plugin</artifactId>
	         <version>3.1.0</version>
	         <configuration>
	            <webResources>
	               <resource>
	                  <directory>src/main/webapp</directory>
	                  <filtering>false</filtering>
	               </resource>
	            </webResources>
	         </configuration>
	      </plugin>
